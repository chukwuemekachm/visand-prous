{
  "name": "visand-prous",
  "version": "1.0.0",
  "description": "visand-prous is an  an online T-Shirt shop which allows users to search, add items to their shopping cart, create order and pay for their orders successfully.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint '**/*js?(x)'",
    "lint:fix": "prettier-eslint '**/*js?(x)' --write",
    "build:client-dev": "rm -rf client/dist && webpack-dev-server --open --env.mode development",
    "build:client-prod": "rm -rf client/dist && webpack --env.mode production",
    "prestart": "npm run build:client-prod",
    "start": "echo 'Start script not specified' && exit 1",
    "dev": "npm run build:client-dev && rm -rf client/dist && webpack-dev-server --env.mode development --open --hot",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "test": "jest --env=jsdom --coverage",
    "watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chukwuemekachm/visand-prous.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chukwuemekachm/visand-prous/issues"
  },
  "homepage": "https://github.com/chukwuemekachm/visand-prous#readme",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-arrow-functions": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.5",
    "compression-webpack-plugin": "^2.0.0",
    "css-loader": "^2.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.14.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.7.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "react-intl": "^2.8.0",
    "react-styleguidist": "^9.0.9",
    "regenerator-runtime": "^0.13.1",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.8.0-alpha.1",
    "react-dom": "^16.8.0-alpha.1",
    "react-router-dom": "^4.3.1",
    "styled-components": "^4.2.0"
  }
}
